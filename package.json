{
  "name": "cplib",
  "version": "1.1.1",
  "description": "It calculates the price of shipping a parcel using canada post",
  "main": "build/index.js",
  "scripts": {
    "prepublishOnly": "npm run build && npm pack",
    "start:dev": "nodemon",
    "test": "mocha -r ts-node/register src/calculate.test.ts src/db/sqlite3.test.ts",
    "int": "mocha -r ts-node/register src/integration/integration.test.ts",
    "recalibrate": "mocha -r ts-node/register src/integration/recalibrate.test.ts",
    "auto": "mocha --timeout 0 -r ts-node/register src/autoload.test.ts",
    "build": "rimraf ./build && tsc && cp -rf src/resources/ build/",
    "start": "npm run build && node build/index.js",
    "sourcetest": "mocha --timeout 0 -r ts-node/register src/source.test.ts",
    "lint": "eslint . --ext .ts --fix",
    "update-data": "npm run sourcetest && cp src/resources/cplib.db src/integration/cplib_int.db && npm run build"
  },
  "keywords": [
    "canada post",
    "cplib",
    "shipping cost",
    "parcel cost"
  ],
  "author": "ana108",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ana108/cplib.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "sinon-stub-promise": "^4.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@types/chai-as-promised": "^7.1.3",
    "@types/follow-redirects": "^1.13.1",
    "@types/sqlite3": "^3.1.7",
    "axios": "^0.21.1",
    "chai-as-promised": "^7.1.1",
    "events": "^3.2.0",
    "fs": "0.0.1-security",
    "node": "^14.19.0",
    "pdf2json": "1.3.1",
    "sinon": "^9.2.0",
    "sqlite3": "^5.0.0",
    "tsc": "^2.0.4",
    "winston": "^3.3.3"
  }
}
